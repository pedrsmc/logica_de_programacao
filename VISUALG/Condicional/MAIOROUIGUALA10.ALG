Algoritmo "maiorouiguala10"

Var
   valor: real

Inicio

   escreva("Digite um número: ")
   leia(valor)

   escreval()

   se (valor > 10.0) entao
      escreva("É MAIOR QUE 10!")

   senao
      se (valor = 10.0) entao
         escreva("VALOR IGUAL A 10!")
      senao
         escreva("NÃO É MAIOR QUE 10!")
      fimse

   fimse

Fimalgoritmo